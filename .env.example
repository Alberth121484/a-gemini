# Slack Configuration
SLACK_BOT_TOKEN=xoxb-your-bot-token
SLACK_APP_TOKEN=xapp-your-app-token
SLACK_SIGNING_SECRET=your-signing-secret

# Google Gemini
GOOGLE_API_KEY=your-gemini-api-key

# OpenAI (for audio transcription/generation)
OPENAI_API_KEY=your-openai-api-key

# Anthropic (optional, for Claude)
ANTHROPIC_API_KEY=your-anthropic-api-key

# Tavily (web search)
TAVILY_API_KEY=your-tavily-api-key

# ConvertAPI (document conversion)
CONVERT_API_KEY=your-convertapi-key

# PostgreSQL
DATABASE_URL=postgresql://user:password@localhost:5432/agentdb
DATABASE_POOL_MIN=10
DATABASE_POOL_MAX=100

# Redis (for caching and rate limiting)
REDIS_URL=redis://localhost:6379/0

# Server Configuration
HOST=0.0.0.0
PORT=8000
WORKERS=4
LOG_LEVEL=INFO

# Rate Limiting
RATE_LIMIT_REQUESTS=100
RATE_LIMIT_WINDOW=60

# Model Configuration
DEFAULT_MODEL=gemini-2.5-pro
IMAGE_MODEL=imagen-4.0-ultra-generate-preview-06-06

# Memory Configuration
CHAT_HISTORY_TABLE=n8n_chat_histories_geminiv2
LOGS_TABLE=logsgemini
CONTEXT_WINDOW_LENGTH=3
